{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "ADF-PURU-DEMO"
		},
		"AzureBlobStorage1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage1'"
		},
		"lsAzureBlobStorage_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'lsAzureBlobStorage'"
		},
		"ls_blob_emp_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_blob_emp'"
		},
		"ls_blob_event_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_blob_event'"
		},
		"ls_blob_event_trigger_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_blob_event_trigger'"
		},
		"ls_blob_training_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_blob_training'"
		},
		"ls_dmf_blob_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_dmf_blob'"
		},
		"ls_event_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_event'"
		},
		"ls_json_blob_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_json_blob'"
		},
		"ls_sqldb_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_sqldb'"
		},
		"ls_sqldb_copy_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_sqldb_copy'"
		},
		"lshremp_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'lshremp'"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ADF-PURU-DEMO_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStorage1_connectionString": {
						"value": "[parameters('AzureBlobStorage1_connectionString')]"
					},
					"lsAzureBlobStorage_connectionString": {
						"value": "[parameters('lsAzureBlobStorage_connectionString')]"
					},
					"ls_blob_emp_connectionString": {
						"value": "[parameters('ls_blob_emp_connectionString')]"
					},
					"ls_blob_event_connectionString": {
						"value": "[parameters('ls_blob_event_connectionString')]"
					},
					"ls_blob_event_trigger_connectionString": {
						"value": "[parameters('ls_blob_event_trigger_connectionString')]"
					},
					"ls_blob_training_connectionString": {
						"value": "[parameters('ls_blob_training_connectionString')]"
					},
					"ls_dmf_blob_connectionString": {
						"value": "[parameters('ls_dmf_blob_connectionString')]"
					},
					"ls_event_connectionString": {
						"value": "[parameters('ls_event_connectionString')]"
					},
					"ls_json_blob_connectionString": {
						"value": "[parameters('ls_json_blob_connectionString')]"
					},
					"ls_sqldb_connectionString": {
						"value": "[parameters('ls_sqldb_connectionString')]"
					},
					"ls_sqldb_copy_connectionString": {
						"value": "[parameters('ls_sqldb_copy_connectionString')]"
					},
					"lshremp_connectionString": {
						"value": "[parameters('lshremp_connectionString')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "ADF-PURU-DEMO_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'ADF-PURU-DEMO_ArmTemplate_0')]"
			]
		}
	]
}