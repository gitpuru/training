{
	"name": "pl_json",
	"properties": {
		"activities": [
			{
				"name": "Copy data1",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [
					{
						"name": "Source",
						"value": "input//emp_json.json"
					},
					{
						"name": "Destination",
						"value": "dbo.emp_json"
					}
				],
				"typeProperties": {
					"source": {
						"type": "JsonSource",
						"storeSettings": {
							"type": "AzureBlobStorageReadSettings",
							"recursive": true,
							"enablePartitionDiscovery": false
						},
						"formatSettings": {
							"type": "JsonReadSettings"
						}
					},
					"sink": {
						"type": "AzureSqlSink",
						"writeBehavior": "insert",
						"sqlWriterUseTableLock": false
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"path": "$['id']"
								},
								"sink": {
									"name": "id",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['Name']"
								},
								"sink": {
									"name": "name",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['skills'][0]"
								},
								"sink": {
									"name": "skills",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['Address']['Country']"
								},
								"sink": {
									"name": "country",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['Address']['City']"
								},
								"sink": {
									"name": "city",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['Address']['Zipcode']"
								},
								"sink": {
									"name": "zip_code",
									"type": "Int32"
								}
							},
							{
								"source": {
									"path": "$['Contact']['Email']"
								},
								"sink": {
									"name": "contact",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['Contact']['Mobile']"
								},
								"sink": {
									"name": "c_num1",
									"type": "String"
								}
							}
						]
					}
				},
				"inputs": [
					{
						"referenceName": "ds_emp_json",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "ds_sql_json",
						"type": "DatasetReference"
					}
				]
			}
		],
		"parameters": {
			"folderpath": {
				"type": "string"
			},
			"filename": {
				"type": "string"
			}
		},
		"annotations": []
	}
}