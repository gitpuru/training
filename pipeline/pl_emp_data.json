{
	"name": "pl_emp_data",
	"properties": {
		"description": "Pipeline to bring employee from source systems",
		"activities": [
			{
				"name": "cp_input_output",
				"description": "Copies data from input to output",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 2,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [
					{
						"name": "Copy_emp",
						"value": "Copy emp data"
					},
					{
						"name": "Source",
						"value": "input//emp_sample.csv"
					},
					{
						"name": "Destination",
						"value": "output//emp_sample.csv"
					}
				],
				"typeProperties": {
					"source": {
						"type": "DelimitedTextSource",
						"storeSettings": {
							"type": "AzureBlobStorageReadSettings",
							"fileListPath": "input/emp_sample1.csv",
							"enablePartitionDiscovery": false
						},
						"formatSettings": {
							"type": "DelimitedTextReadSettings"
						}
					},
					"sink": {
						"type": "DelimitedTextSink",
						"storeSettings": {
							"type": "AzureBlobStorageWriteSettings"
						},
						"formatSettings": {
							"type": "DelimitedTextWriteSettings",
							"quoteAllText": true,
							"fileExtension": ".txt"
						}
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"name": "1001",
									"type": "String",
									"physicalType": "String"
								},
								"sink": {
									"name": "abc",
									"type": "String",
									"physicalType": "String"
								}
							},
							{
								"source": {
									"name": "abc",
									"type": "String",
									"physicalType": "String"
								},
								"sink": {
									"name": "1001",
									"type": "String",
									"physicalType": "String"
								}
							},
							{
								"source": {
									"name": "1000",
									"type": "String",
									"physicalType": "String"
								},
								"sink": {
									"name": "1000",
									"type": "String",
									"physicalType": "String"
								}
							}
						],
						"typeConversion": true,
						"typeConversionSettings": {
							"allowDataTruncation": true,
							"treatBooleanAsNumber": false
						}
					}
				},
				"inputs": [
					{
						"referenceName": "ds_emp_csv",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "target_emp_csv",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	}
}